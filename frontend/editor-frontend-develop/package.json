{
  "name": "xcdt",
  "version": "0.0.1",
  "scripts": {
    "dev": "vigour",
    "dev:rs": "nodemon",
    "build": "vigour build",
    "build:time": "vigour build --bundle-time",
    "build:size": "vigour build --bundle-size",
    "preview": "vigour preview",
    "prepare": "npx simple-git-hooks",
    "cz": "git-cz",
    "release": "standard-version",
    "lint": "eslint --fix src/**/*.{ts,tsx} && prettier --write src && stylelint --fix src/**/*.{less,css}"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not IE 11",
    "not op_mini all"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{less,css}": "stylelint --fix"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz.config.js"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@antv/layout": "^1.2.14-beta.8",
    "@antv/x6": "^2.18.1",
    "@antv/x6-plugin-clipboard": "^2.1.6",
    "@antv/x6-plugin-dnd": "^2.1.1",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-keyboard": "^2.2.3",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@monaco-editor/react": "^4.7.0",
    "@renzp/storage": "^1.0.1",
    "@renzp/utils": "^0.3.11",
    "@turf/hex-grid": "^7.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ahooks": "^3.8.4",
    "antd": "^5.23.1",
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "browserify-zlib": "^0.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "https-browserify": "^1.0.0",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.2",
    "mqtt": "^5.10.3",
    "nanoid": "^5.1.5",
    "nprogress": "^0.2.0",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^7.1.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-resizable": "^3.0.8",
    "@vigour/eslint-config": "^0.0.5",
    "@vigour/scripts": "^0.1.23",
    "commitizen": "^4.3.1",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.4.0",
    "eslint": "^8.46.0",
    "lint-staged": "^15.4.0",
    "nodemon": "^3.0.1",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "standard-version": "^9.5.0",
    "stylelint": "^16.13.2",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.3"
  }
}
